<div id="<%= dom_id user %>">
  <p>
    <strong>Email:</strong>
    <%= user.get_email %>
  </p>

  <p>
    <strong>UIN:</strong>
    <%= user.uin %>
  </p>

  <p>
    <strong>Classification:</strong>
    <%= user.get_classification %>
  </p>

  <p>
    <strong><%= user.admin ? 'Has' : 'Does not have' %> admin privileges</strong>
  </p>
</div>